<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/signInFragment">

    <fragment
        android:id="@+id/signInFragment"
        android:name="uz.sultonbek1547.hackathonproject2024_innovatex.ui.presentation.sign.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpTwoFragment"
            app:destination="@id/signUpTwoFragment" />
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="uz.sultonbek1547.hackathonproject2024_innovatex.ui.presentation.sign.SignUpFragment"
        android:label="SignUpFragment" >
        <action
            android:id="@+id/action_signUpFragment_to_signUpTwoFragment"
            app:destination="@id/signUpTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpTwoFragment"
        android:name="dev.abdujabbor.modeltest.screens.sign.SignUpTwoFragment"
        android:label="SignUpTwoFragment" />
</navigation>